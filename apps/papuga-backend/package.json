{
  "name": "@pck/papuga-backend",
  "version": "0.0.1",
  "description": "Papuga backend package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tspc --build",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "tsoa:routes": "tsoa routes",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d .\\src\\data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate .\\src\\migrations\\GeneratedMigration -d .\\src\\data-source.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^5.1.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
