{
  "name": "@pck/papuga-backend",
  "version": "0.0.1",
  "description": "Papuga backend package",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npm run tsoa && tspc --build",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "tsoa": "tsoa routes && tsoa spec",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d .\\src\\data-source.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate .\\src\\migrations\\GeneratedMigration -d .\\src\\data-source.ts"
  },
  "dependencies": {
    "@mechanicalhuman/bunyan-pretty": "^3.1.0",
    "axios": "^1.6.2",
    "bunyan": "^1.8.15",
    "connect-redis": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "jose": "^5.1.3",
    "node-schedule": "^2.1.1",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-errors": "^2.0.4",
    "@types/node-schedule": "^2.1.5",
    "@types/swagger-ui-express": "^4.1.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
